{
    "applicationId": "app_group_v1",
    "alias": "mytestapp1",
    "components": {
        "cartridges": [
            {
                "type": "tomcat",
                "cartridgeMin": 1,
                "cartridgeMax": 2,
                "subscribableInfo": {
                    "alias": "mytomcat",
                    "autoscalingPolicy": "autoscale_policy_1"
                }
            },
            {
                "type": "php",
                "cartridgeMin": 2,
                "cartridgeMax": 4,
                "subscribableInfo": {
                    "alias": "myphp",
                    "autoscalingPolicy": "autoscale_policy_1"
                }
            }
        ],
        "dependencies": {
            "scalingDependants": [
                "cartridge.mytomcat, cartridge.myphp"
            ],
            "terminationBehaviour": "terminate-none"
        }
    }
}
